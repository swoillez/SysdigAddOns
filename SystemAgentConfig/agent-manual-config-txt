echo customerid: b7b5cf22-24c7-4da3-a054-f0e86bcb4794 >> /opt/draios/etc/dragent.default.yaml

kubectl get secret `kubectl get sa/sysdig-agent -n sysdig-agent -o yaml | grep token | awk '{print $3}'` -o yaml -n sysdig-agent | grep token: | awk '{print $2}' | base64 -d > /opt/draios/etc/sysdig-agent-bearer-token